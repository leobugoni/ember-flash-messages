{{#each queue.nonTimedMessages}}
  {{type}}
  {{content}}
{{/each}}

{{#with currentMessage}}

  <dt>
    {{#if iconClass}}
      <span {{bind-attr class="iconClass"}}></span>
    {{/if}}

    <span data-test="notify-type">{{type}}</span>
  </dt>

  <dd data-test="notify-content">
    {{content}}
  </dd>

{{/with}}
